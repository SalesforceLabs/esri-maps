<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Activity_Approval_Status__c</fullName>
    <description>This field is used to display the color on the basis of approval or rejection of related activities/tasks.</description>
    <externalId>false</externalId>
    <formula>CASE(Activity_Status__c,
 &apos;Approved&apos;, IMAGE(&apos;/resource/Green&apos;,&apos;GREEN&apos;,15,15),
 &apos;Rejected&apos;, IMAGE(&apos;/resource/Red&apos;,&apos;RED&apos;,15,15),
 TEXT(Status) 
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Activity Approval Status</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>

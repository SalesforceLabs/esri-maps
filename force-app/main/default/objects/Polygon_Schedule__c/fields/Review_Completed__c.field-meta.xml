<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Review_Completed__c</fullName>
    <description>This is to capture whether the Reviewer has completed the review of the record.</description>
    <externalId>false</externalId>
    <formula>OR(
ISPICKVAL( Status__c, &apos;Approved&apos;),
ISPICKVAL(Status__c, &apos;Rejected&apos;)
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Review Completed</label>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
